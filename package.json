{
  "name": "saleor-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm generate && NODE_OPTIONS='--inspect' next dev",
    "build": "pnpm generate && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "graphql-codegen --require dotenv/config"
  },
  "saleorApp": {
    "name": "My Saleor App",
    "logo": "/public/logo.png",
    "shortDescription": "Bare-bones boilerplate for writing Saleor Apps with Next.js & MacawUI",
    "sourceUrl": "https://github.com/",
    "vendor": "Saleor Commerce",
    "links": [
      {
        "type": "developerWebsite",
        "href": "https://saleor.io/"
      },
      {
        "type": "mailContact",
        "href": "mailto:info@saleor.io"
      },
      {
        "type": "privacyPolicy",
        "href": "https://saleor.io/legal/privacy/"
      }
    ],
    "description": "Saleor App is the fastest way of extending Saleor with custom logic using asynchronous and synchronous webhooks (and vast Saleor's API).",
    "images": [
      "/public/image.png"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@saleor/app-bridge": "^0.1.9",
    "@saleor/app-sdk": "^0.5.0",
    "@saleor/macaw-ui": "^0.6.2",
    "@urql/exchange-auth": "^0.1.7",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "next": "12.2.0",
    "node-fetch": "^3.2.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "retes": "^0.28.0",
    "urql": "^2.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-urql": "^3.5.12",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.0",
    "graphqurl": "^1.0.1",
    "postcss": "^8.4.14",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.1.4",
    "typescript": "4.7.4"
  }
}
